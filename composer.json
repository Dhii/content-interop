{
  "name": "dhii/content-interop",
  "description": "Interop interfaces for content",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Dhii",
      "role": "Organization"
    },
    {
      "name": "Anton Ukhanev",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "psr/container": "^1.0",
    "dhii/data-identifiable-interface": "^0.1",
    "dhii/human-readable-interface": "^0.1",
    "dhii/stringable-interface": "^0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0 | ^7.0",
    "dhii/containers": "^0.1"
  },
  "autoload": {
    "psr-4": {
      "Dhii\\Content\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dhii\\Content\\Test\\Unit\\": "tests/unit"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-task/initial-interfaces": "0.1.x-dev"
    }
  }
}
